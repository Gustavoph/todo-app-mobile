{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["styled-components-react-native"],
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@routes/*": ["./src/routes/*"],
      "@screens/*": ["./src/screens/*"],
      "@storage/*": ["./src/storage/*"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"],
      "@themes/*": ["./src/themes/*" ],
      "@dtos/*": ["./src/dtos/*" ],
      "@hooks/*": ["./src/hooks/*" ],
      "@contexts/*": ["./src/contexts/*" ]
    }
  }
}